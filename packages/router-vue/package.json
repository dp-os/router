        "@gez/lint": "3.0.0-beta.49",
        "@gez/router": "0.0.64",
        "@types/node": "22.9.0",
        "@vitest/coverage-v8": "2.1.5",
        "stylelint": "16.10.0",
        "typescript": "5.7.3",
        "unbuild": "2.0.0",
        "vitest": "2.1.5",
        "vue": "^2.7.8",
        "@biomejs/biome": "1.9.4"
